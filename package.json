{
  "author": "zbryikt",
  "name": "servebase",
  "license": "MIT",
  "description": "base server for constructing services",
  "version": "0.0.1",
  "homepage": "https://github.com/plotdb/servebase",
  "repository": {
    "type": "git",
    "url": "https://github.com/plotdb/servebase"
  },
  "engines": {
    "node": ">=15.11.0",
    "npm": ">=7.0.15"
  },
  "workspaces": [
    "module/auth",
    "module/pugext"
  ],
  "scripts": {
    "postinstall": "./node_modules/.bin/fedep",
    "start": "NODE_ENV=production ./start",
    "dev": "./start",
    "try": "./node_modules/.bin/lsc log.ls",
    "prebuild": "node_modules/.bin/lsc -co .engine engine",
    "config": "cd config; ./build",
    "bootstrap": "npx bootstrap.build -c config/bootstrap/ -o static/assets/lib/bootstrap.custom/main/",
    "docker-start": "docker-compose -f config/docker/compose.yaml up",
    "docker-stop": "docker-compose -f config/docker/compose.yaml down",
    "docker-build": "docker-compose -f config/docker/compose.yaml build",
    "docker-db": "docker-compose -f config/docker/compose.yaml up -d db",
    "cannon": "cd tool; ./cannon",
    "test": "npx mocha --require livescript ./test/index.ls",
    "coverage": "npx nyc --reporter=text npm run test"
  },
  "devDependencies": {
    "colors": "^1.3.3",
    "express": "=4.16.4",
    "stylus": "^0.54.5",
    "uglify-js": "=3.2.0",
    "uglifycss": "0.0.29"
  },
  "dependencies": {
    "@loadingio/debounce.js": "0.0.1",
    "@loadingio/ldc": "0.0.2",
    "@loadingio/ldquery": "^1.1.3",
    "@loadingio/transition.css": "2.0.1",
    "@plotdb/srcbuild": "^0.0.10",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bootstrap.build": "github:plotdb/bootstrap.build",
    "bootstrap.ldui": "github:loadingio/bootstrap.ldui#396fc6c23c6319348a461a35e1cf181b9e6c1f90",
    "bootstrap.native": "^3.0.10",
    "chokidar": "^3.4.2",
    "csurf": "^1.11.0",
    "curegex": "0.0.3",
    "express-list-endpoints": "^5.0.0",
    "express-session": "^1.17.1",
    "fedep": "^0.0.12",
    "i18next": "^19.8.3",
    "i18next-fs-backend": "^1.0.7",
    "i18next-http-middleware": "^3.0.6",
    "js-yaml": "^3.14.0",
    "jsonwebtoken": "^8.5.1",
    "ldbutton": "^1.0.2",
    "ldcover": "^1.3.1",
    "ldcvmgr": "~0.0.3",
    "lderror": "^0.0.5",
    "ldfolder": "^0.0.4",
    "ldform": "^0.0.1",
    "ldiconfont": "^0.1.5",
    "ldloader": "^1.1.1",
    "ldnotify": "0.0.1",
    "ldtab": "^0.1.0",
    "ldview": "0.0.2",
    "livescript": "^1.6.0",
    "ls-lint": "^0.1.2",
    "marked": "^2.0.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-line-auth": "^0.2.9",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "pg-pool": "^3.2.1",
    "pino": "^6.6.0",
    "pino-http": "^5.2.0",
    "pino-pretty": "^4.2.1",
    "proxise": "^0.1.2",
    "pug": "^3.0.0",
    "re2": "^1.15.9",
    "redis": "^3.0.2",
    "redis-clustr": "^1.7.0",
    "require-reload": "^0.2.2",
    "template-text": "^0.1.2",
    "zmgr": "^0.0.3"
  },
  "frontendDependencies": {
    "root": "static/assets/lib",
    "modules": [
      "bootstrap.ldui",
      "bootstrap.native",
      "@loadingio/ldquery",
      "@loadingio/debounce.js",
      "@loadingio/transition.css",
      "@loadingio/ldc",
      "curegex",
      "proxise",
      "ldiconfont",
      "ldcvmgr",
